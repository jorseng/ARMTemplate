{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {},
  "variables": {
    "ARMTest-VnetPrefix": "10.0.0.0/16",
    "ARMTest-VnetSubnet1Name": "Subnet-1",
    "ARMTest-VnetSubnet1Prefix": "10.0.0.0/24",
    "ARMTest-VnetSubnet2Name": "Subnet-2",
    "ARMTest-VnetSubnet2Prefix": "10.0.1.0/24"},
  "resources": [
    {
      "name": "ARMTest-Vnet",
      "type": "Microsoft.Network/virtualNetworks",
      "location": "[resourceGroup().location]",
      "apiVersion": "2016-03-30",
      "dependsOn": [ ],
      "tags": {
        "displayName": "ARMTest-Vnet"
      },
      "properties": {
        "addressSpace": {
          "addressPrefixes": [
            "[variables('ARMTest-VnetPrefix')]"
          ]
        },
        "subnets": [
          {
            "name": "[variables('ARMTest-VnetSubnet1Name')]",
            "properties": {
              "addressPrefix": "[variables('ARMTest-VnetSubnet1Prefix')]"
            }
          },
          {
            "name": "[variables('ARMTest-VnetSubnet2Name')]",
            "properties": {
              "addressPrefix": "[variables('ARMTest-VnetSubnet2Prefix')]"
            }
          }
        ]
      }
    }],
  "outputs": {}
}
