{

  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",

  "contentVersion": "1.0.0.0",

  "parameters": {
    "location": {
      "defaultValue": "Southeast Asia",
      "type": "string"
    },
    "ASAdmin": {
      "type": "string"
    },
    "StorageAccountType": {
      "type": "string",
      "defaultValue": "Standard_LRS",
      "allowedValues": [
        "Standard_LRS",
        "Standard_ZRS",
        "Standard_GRS",
        "Standard_RAGRS",
        "Premium_LRS"
      ]
    }
  },

  "variables": {
    "ARMTest-StorageAccountName": "ARMTest-SA"
  },

  "resources": [
    {
      "name": "[variables('armtestsaName')]",
      "type": "Microsoft.Storage/storageAccounts",
      "location": "[parameters('location')]",
      "apiVersion": "2016-01-01",
      "sku": {
        "name": "[parameters('StorageAccountType')]"
      },
      "dependsOn": [ ],
      "tags": {
        "displayName": "armtestsa"
      },
      "kind": "Storage"
    }
  ]
}